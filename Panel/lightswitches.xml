     <button>0</button>
     <x>-230</x>
     <y>30</y>
     <w>20</w>
     <h>30</h>
     <binding>
       <command>property-toggle</command>
       <property>/controls/switches/master-panel</property>
       <step>1</step>
       <min>0</min>
       <max>1</max>
       <wrap>true</wrap>
     </binding>
   </action>

