<?xml version="1.0"?>

<!DOCTYPE PropertyList SYSTEM "panel.dtd">

<PropertyList>

 <name>B1900D Overhead Switches</name>
 <background>Aircraft/b1900d/Panel/transparent-bg.rgb</background>
 <w>512</w>
 <h>384</h>

<instruments>
<instrument>
  <name>panel hotspots</name>
  <x>256</x>
  <y>192</y>
  <w>512</w>
  <h>384</h>
  <w-base>512</w-base>
  <h-base>384</h-base>

