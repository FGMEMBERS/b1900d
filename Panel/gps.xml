<?xml version="1.0"?>

<!--
Honeywell Bendix/King KLN-90B GPS
-->

<PropertyList>

  <name>KLN-90B</name>
  <w-base>512</w-base>
  <h-base>176</h-base>


 <layers>
   <layer>
     <name>bezel</name>
     <texture>
       <path>Aircraft/b1900d/Models/gps.rgb</path>
       <x1>0.0</x1>
       <y1>0.3</y1>
       <x2>1.0</x2>
       <y2>1.0</y2>
     </texture>
     <w>512</w>
     <h>176</h>
   </layer>
</layers>

<actions>

  <action>
    <name>select left mode down</name>
    <button>0</button>
    <x>-225</x>
    <y>-52</y>
    <w>18</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.lh_menu_update(1)</script>
    </binding>
  </action>
   
  <action>
    <name>select left mode up</name>
    <button>0</button>
    <x>-181</x>
    <y>-52</y>
    <w>18</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.lh_menu_update(2)</script>
    </binding>
  </action>

  <action>
    <name>select left mode down fine</name>
    <button>0</button>
    <x>-207</x>
    <y>-52</y>
    <w>13</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.lh_menu_update(3)</script>
    </binding>
  </action>
   
  <action>
    <name>select left mode up fine </name>
    <button>0</button>
    <x>-194</x>
    <y>-52</y>
    <w>13</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.lh_menu_update(4)</script>
    </binding>
  </action>

   
  <action>
    <name>select right mode down</name>
    <button>0</button>
    <x>158</x>
    <y>-52</y>
    <w>18</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.rh_menu_update(1)</script>
    </binding>
  </action>
   
  <action>
    <name>select right mode up</name>
    <button>0</button>
    <x>202</x>
    <y>-52</y>
    <w>18</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.rh_menu_update(2)</script>
    </binding>
  </action>
   
  <action>
    <name>select right mode down fine</name>
    <button>0</button>
    <x>176</x>
    <y>-52</y>
    <w>13</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.rh_menu_update(3)</script>
    </binding>
  </action>
   
  <action>
    <name>select right mode up fine </name>
    <button>0</button>
    <x>189</x>
    <y>-52</y>
    <w>13</w>
    <h>25</h>
    <binding>
      <command>nasal</command>
      <script>systems.rh_menu_update(4)</script>
    </binding>
  </action>

   <action>
     <name>get nearest airport</name>
     <button>0</button>
     <x>167</x>
     <y>-2</y>
     <w>44</w>
     <h>32</h>
     <binding>
       <command>nasal</command>
       <script>
         node =props.globals.getNode("/instrumentation/gps/wp/wp[1]/get-nearest-airport", 1);
         node.setValue(1);
       </script>
     </binding>
   </action>

     <action>
       <name>power on-off</name>
       <button>0</button>
       <x>177</x>
       <y>41</y>
       <w>28</w>
       <h>28</h>
       <binding>
         <command>nasal</command>
         <script>systems.gpspower()</script>
       </binding>
       
     </action>

     <action>
       <name>enter button</name>
       <button>0</button>
       <x>90</x>
       <y>-71</y>
       <w>34</w>
       <h>20</h>
       <binding>
         <command>nasal</command>
         <script></script>
       </binding>
     </action>

     <action>
       <name>clr button</name>
       <button>0</button>
       <x>35</x>
       <y>-71</y>
       <w>34</w>
       <h>20</h>
       <binding>
         <command>nasal</command>
         <script></script>
       </binding>
     </action>

     <action>
       <name>direct_to button</name>
