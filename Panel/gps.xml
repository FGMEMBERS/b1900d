       <x>-75</x>
       <y>-71</y>
       <w>34</w>
       <h>20</h>
       <binding>
         <command>nasal</command>
         <script></script>
       </binding>
     </action>

     <action>
       <name>msg button</name>
       <button>0</button>
       <x>-128</x>
