   <action>
     <name>get nearest airport</name>
     <button>0</button>
     <x>48</x>
     <y>0</y>
     <w>10</w>
     <h>7</h>
     <binding>
       <command>nasal</command>
       <script>
         setprop("/instrumentation/gps/wp/wp[1]/get-nearest-airport", 1);
       </script>
     </binding>
   </action>

     <action>
       <name>power on-off</name>
       <button>0</button>
       <x>50</x>
       <y>10</y>
       <w>8</w>
       <h>8</h>
       <binding>
         <command>property-toggle</command>
         <property>/instrumentation/gps/serviceable</property>
       </binding>
       
     </action>

     <action>
       <name>enter button</name>
       <button>0</button>
       <x>25</x>
