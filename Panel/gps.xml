  <action>
    <name>select left mode down</name>
    <button>1</button>
    <x>-63</x>
    <y>-21</y>
    <w>18</w>
    <h>10</h>
    <binding>
      <command>nasal</command>
      <script>systems.lh_menu_update(1)</script>
    </binding>
  </action>
   
  <action>
    <name>select left mode up</name>
