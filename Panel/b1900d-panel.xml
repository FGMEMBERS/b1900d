<?xml version="1.0"?>

<!DOCTYPE PropertyList SYSTEM "panel.dtd">

<PropertyList>

 <name>B1900D Panel</name>
 <background>Aircraft/b1900d/Panel/transparent-bg.rgb</background>
 <w>1024</w>
 <h>768</h>

<instruments>
   
  <!-- radio stack -->  
  <instrument include="comradio.xml">
    <params>
      <selected-mhz>/instrumentation/comm[0]/frequencies/selected-mhz</selected-mhz>
      <standby-mhz>/instrumentation/comm[0]/frequencies/standby-mhz</standby-mhz>
    </params>
    <name>Com1</name>
    <x>445</x>
    <y>272</y>
    <w>48</w>
    <h>54</h>
  </instrument>
  
  <instrument include="navradio.xml">
    <params>
      <selected-mhz>/instrumentation/nav[0]/frequencies/selected-mhz</selected-mhz>
      <standby-mhz>/instrumentation/nav[0]/frequencies/standby-mhz</standby-mhz>
    </params>
    <name>Nav1</name>
    <x>500</x>
    <y>272</y>
    <w>48</w>
    <h>54</h>
  </instrument>
  

  <instrument include="navradio.xml">
    <params>
      <selected-mhz>/instrumentation/nav[1]/frequencies/selected-mhz</selected-mhz>
      <standby-mhz>/instrumentation/nav[1]/frequencies/standby-mhz</standby-mhz>
    </params>
    <name>Nav2</name>
    <x>606</x>
    <y>272</y>
    <w>48</w>
    <h>54</h>
  </instrument>

  <instrument include="comradio.xml">
    <params>
      <selected-mhz>/instrumentation/comm[1]/frequencies/selected-mhz</selected-mhz>
      <standby-mhz>/instrumentation/comm[1]/frequencies/standby-mhz</standby-mhz>
    </params>
    <name>Com2</name>
    <x>660</x>
    <y>272</y>
    <w>48</w>
    <h>54</h>
  </instrument>

  <instrument include="adf-radio.xml">
    <name>ADF</name>
    <x>445</x>
    <y>174</y>
    <w>48</w>
    <h>54</h>
  </instrument>
  
  <instrument include="gps.xml">
    <name>GPS display</name>
    <x>615</x>
    <y>222</y>
    <w>110</w>
    <h>40</h>
  </instrument>

<instrument>
  <name>panel hotspots</name>
  <x>512</x>
  <y>384</y>
  <w>1024</w>
  <h>768</h>
  <w-base>1024</w-base>
  <h-base>768</h-base>

