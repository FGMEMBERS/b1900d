  <!--             Altimeter              -->  
  
<action>
  <name>calibrate fine</name>
  <button>0</button>
  <x>-214</x>
  <y>-124</y>
  <w>8</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>-0.01</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
  </binding>
</action>

<action>
  <name>calibrate coarse</name>
  <button>1</button>
  <x>-214</x>
  <y>-124</y>
  <w>8</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>-0.1</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
  </binding>
</action>

<action>
  <name>calibrate fine</name>
  <button>0</button>
  <x>-206</x>
  <y>-124</y>
  <w>8</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>0.01</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
  </binding>
</action>

<action>
  <name>calibrate coarse</name>
  <button>1</button>
  <x>-206</x>
  <y>-124</y>
  <w>8</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/altimeter/setting-inhg</property>
    <step>0.1</step>
    <min>26.0</min>
    <max>33.0</max>
    <wrap>false</wrap>
  </binding>
</action>
  

<!--           Altitude Set                           -->
  
<action>
  <name>reset altitude</name>
  <button>0</button>
  <x>-84</x>
  <y>-158</y>
  <w>14</w>
  <h>14</h>
  <binding>
    <command>property-assign</command>
    <property>/autopilot/settings/target-altitude-ft</property>
    <value>0.0</value>
  </binding>
</action>


<action>
  <name>decrease altitude 100</name>
  <button>0</button>
  <x>-36</x>
  <y>-166</y>
  <w>6</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/autopilot/settings/target-altitude-ft</property>
    <step>-100</step>
    <min>0</min>
    <max>99000</max>
    <wrap>false</wrap>
  </binding>
</action>

<action>
  <name>increase altitude 100</name>
  <button>0</button>
  <x>-30</x>
  <y>-166</y>
  <w>6</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/autopilot/settings/target-altitude-ft</property>
    <step>100</step>
    <min>0</min>
    <max>99000</max>
    <wrap>false</wrap>
  </binding>
</action>


<action>
  <name>decrease altitude 1000</name>
  <button>1</button>
  <x>-36</x>
  <y>-166</y>
  <w>6</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/autopilot/settings/target-altitude-ft</property>
    <step>-1000</step>
    <min>0</min>
    <max>99000</max>
    <wrap>false</wrap>
  </binding>
</action>

<action>
  <name>increase altitude 1000</name>
  <button>1</button>
  <x>-30</x>
  <y>-166</y>
  <w>6</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/autopilot/settings/target-altitude-ft</property>
    <step>1000</step>
    <min>0</min>
    <max>99000</max>
    <wrap>false</wrap>
  </binding>
</action>

<!--                   Analog Clock               -->

<action>
  <name>adjust hour up</name>
  <button>1</button>
  <x>-462</x>
  <y>-138</y>
  <w>10</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/clock/offset-sec</property>
    <step>3600</step>
    <min>0</min>
    <max>43200</max>
    <wrap>true</wrap>
  </binding>
</action>

<action>
  <name>adjust hour down</name>
  <button>1</button>
  <x>-472</x>
  <y>-138</y>
  <w>10</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/clock/offset-sec</property>
    <step>-3600</step>
    <min>0</min>
    <max>43200</max>
    <wrap>1</wrap>
  </binding>
</action>

<action>
  <name>adjust minute up</name>
  <button>0</button>
  <x>-462</x>
  <y>-138</y>
  <w>10</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/clock/offset-sec</property>
    <step>60</step>
    <min>0</min>
    <max>43200</max>
    <wrap>true</wrap>
  </binding>
</action>

<action>
  <name>adjust minute down</name>
  <button>1</button>
  <x>-472</x>
  <y>-138</y>
  <w>10</w>
  <h>8</h>
  <binding>
    <command>property-adjust</command>
    <property>/instrumentation/clock/offset-sec</property>
    <step>60</step>
    <min>0</min>
    <max>43200</max>
    <wrap>1</wrap>
  </binding>
</action>







</actions>

</instrument>

</instruments>

</PropertyList>