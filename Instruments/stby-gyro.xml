<?xml version="1.0"?>
<PropertyList>

    <path>stby-gyro.ac</path>

    <animation>
        <type>material</type>
        <object-name>gyro-face</object-name>
        <emission>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
            <factor-prop>/systems/electrical/outputs/efis-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>Stby-gyro</object-name>
        <emission>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/panel-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>texmultiple</type>
        <object-name>gyro-face</object-name>
        <transform>
            <property>/orientation/pitch-deg</property>
            <subtype>textranslate</subtype>
            <factor>0.004296</factor>
            <axis>
                <x>0</x>
                <y>1</y>
                <z>0</z>
            </axis>
        </transform>
        <transform>
            <property>/orientation/roll-deg</property>
            <subtype>texrotate</subtype>
            <center>
                <x>0.25</x>
                <y>0.5</y>
            </center>
            <axis>
                <x>0</x>
                <y>0</y>
                <z>-1</z>
            </axis>
        </transform>
    </animation>

</PropertyList>