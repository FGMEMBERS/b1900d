<?xml version="1.0"?>
<PropertyList>

    <path>fuelqty.ac</path>

    <animation>
        <type>material</type>
        <object-name>Fuelface</object-name>
        <object-name>FQneedle</object-name>
        <object-name>FQneedle.1</object-name>
        <emission>
            <red>0.035</red>
            <green>0.012</green>
            <blue>0.007</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>FuelPanel</object-name>
        <object-name>Fswitch.1</object-name>
        <object-name>Fswitch.2</object-name>
        <object-name>Fswitch.3</object-name>
        <object-name>Fswitch.4</object-name>
        <object-name>Fswitch.5</object-name>
        <object-name>Fswitch.6</object-name>
        <emission>
            <red>0.01</red>
            <green>0.01</green>
            <blue>0.01</blue>
            <factor-prop>controls/lighting/panel-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.1</object-name>
        <property>/consumables/fuel/tank/level-lbs</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>0.0</dep>
            </entry>
            <entry>
                <ind>2000.0</ind><dep>240.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.045</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle.1</object-name>
        <property>/consumables/fuel/tank[2]/level-lbs</property>
        <interpolation>
            <entry>
                <ind>0.0</ind><dep>0.0</dep>
            </entry>
            <entry>
                <ind>2000.0</ind><dep>240.0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>-0.045</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle</object-name>
        <property>/consumables/fuel/tank[1]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2000.0</ind><dep>240.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.045</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>FQneedle</object-name>
        <property>/consumables/fuel/tank[3]/level-lbs</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>2000.0</ind><dep>240.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1.0</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.045</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

</PropertyList>
