<?xml version="1.0"?>

<PropertyList>

 <path>trbgauge.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>TR.needle</object-name>
    <property>/engines/engine[0]/n2</property>
    <interpolation>
      <entry>
        <ind>      0.0</ind>
        <dep>   0.0</dep>
      </entry>
      <entry>
        <ind>   110.0</ind>
        <dep> 248.0</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0.0</z>
    </axis>
  </animation>

 <animation>
  <type>textranslate</type>
  <object-name>trb.1</object-name>
  <property>/engines/engine[0]/n2</property>
  <factor>0.001</factor>
  <step>100</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>  

 <animation>
  <type>textranslate</type>
  <object-name>trb.2</object-name>
  <property>/engines/engine[0]/n2</property>
  <factor>0.01</factor>
  <step>10</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>  


 <animation>
  <type>textranslate</type>
  <object-name>trb.3</object-name>
  <property>/engines/engine[0]/n2</property>
  <factor>0.1</factor>
  <step>1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>  

 <animation>
  <type>textranslate</type>
  <object-name>trb.4</object-name>
  <property>/engines/engine[0]/n2</property>
  <factor>1.0</factor>
  <step>0.1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation> 
 
   <animation>
  <type>material</type>
    <object-name>TRFace</object-name>
    <global type="bool">true</global>
  <emission>
    <red>1.0</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/engines-norm</factor-prop>
  </emission>
</animation>

</PropertyList>
