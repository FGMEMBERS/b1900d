<?xml version="1.0"?>

<PropertyList>

 <path>asi300gauge.ac</path>

  
   <animation>
  <type>material</type>
    <object-name>ASIface</object-name>
    <global type="bool">true</global>
  <emission>
    <red>1.0</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

 <animation>
  <type>material</type>
    <object-name>ASIbezel</object-name>
    <global type="bool">true</global>
  <emission>
    <red>0.2</red>
    <green>0.2</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>


<animation>
    <type>rotate</type>
    <object-name>ASI.needle</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry>
        <ind>  0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind> 40.0</ind>
        <dep>10.0</dep>
      </entry>
      <entry>
        <ind> 100.0</ind>
        <dep>88.0</dep>
      </entry>
      <entry>
        <ind> 170.0</ind>
        <dep>178.0</dep>
      </entry>
      <entry>
        <ind> 240.0</ind>
        <dep>268.0</dep>
      </entry> 
     <entry>
        <ind> 300.0</ind>
        <dep>333.0</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
  </animation>
    
</PropertyList>
