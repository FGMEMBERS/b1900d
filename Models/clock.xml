<?xml version="1.0"?>

<PropertyList>

 <path>clock.ac</path>


 <animation>
  <type>material</type>
    <object-name>ScHand</object-name>
    <global type="bool">true</global>
  <emission>
    <red>1.0</red>
    <green>0.5</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

 <animation>
  <type>material</type>
    <object-name>Clock</object-name>
    <global type="bool">true</global>
  <emission>
    <red>0.2</red>
    <green>0.2</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>


    <animation>
  <type>rotate</type>
  <object-name>ScHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>6</factor>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0</z>
    </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>MnHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>0.1</factor>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0.0</z>
    </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>HrHand</object-name>
  <property>/instrumentation/clock/indicated-sec</property>
  <factor>0.008333</factor>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0.0</z>
    </axis>
 </animation>




</PropertyList>
