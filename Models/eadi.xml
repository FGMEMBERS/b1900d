<?xml version="1.0"?>

<PropertyList>

 <path>efis84-eadi.ac</path>

 <animation>
   <type>select</type>
   <object-name>FDbars</object-name>
   <condition>
     <or>
       <not-equals>
         <property>/instrumentation/flightdirector/lnav</property>
         <value>0</value>
       </not-equals>
       <not-equals>
         <property>/instrumentation/flightdirector/vnav</property>
         <value>0</value>
       </not-equals>
     </or>
   </condition>
 </animation>



<animation>
  <type>translate</type>
  <object-name>slipball</object-name>
  <property>/instrumentation/slip-skid-ball/indicated-slip-skid</property>
  <min>-1.0</min>
  <max>1.0</max>
  <interpolation>
    <entry>
      <ind>-1.0</ind>
      <dep>-0.02</dep>
    </entry>
    <entry>
      <ind>1.0</ind>
      <dep>0.02</dep>
    </entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>1.0</y>
    <z>0.0</z>
  </axis>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.095</z-m>
  </center>
</animation>
   
  <animation>
   <type>translate</type>
   <object-name>FDbars</object-name>
    <property>/orientation/pitch-deg</property>
<factor>0.00087</factor>
<axis>
     <x>0.0</x>
     <y>0.0</y>
     <z>-1.0</z>
   </axis>
   <center>
     <x-m>0.000</x-m>
     <y-m>0</y-m>
     <z-m>0.001</z-m>
   </center>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>FDbars</object-name>
    <property>/autopilot/internal/target-roll-deg</property>
   <axis>
     <x>-1.0</x>
     <y>0.0</y>
     <z>0.0</z>
   </axis>
   <center>
     <x-m>0.0</x-m>
     <y-m>0</y-m>
     <z-m>0.007</z-m>
   </center>
 </animation>
  

  <animation>
  <type>texmultiple</type>
    <object-name>AIback</object-name>
  <transform>
    <property>/orientation/pitch-deg</property>
    <subtype>textranslate</subtype>
    <factor>0.004296</factor>
    <axis>
     <x>0</x>
     <y>1</y>
     <z></z>
   </axis>
  </transform>
  <transform>
    <property>/orientation/roll-deg</property>
    <subtype>texrotate</subtype>
    <center>
     <x>0.25</x>
     <y>0.5</y>
   </center>
    <axis>
     <x>0</x>
     <y>0</y>
     <z>-1</z>
   </axis>
  </transform>
 </animation>              
  
<animation>
  <type>material</type>
    <object-name>EADI.face</object-name>
    <emission>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>/controls/lighting/eadi-ehsi-norm</factor-prop>
  </emission>
</animation>

 <animation>
  <type>material</type>
    <object-name>EADI.bezel</object-name>
    <emission>
    <red>0.2</red>
    <green>0.2</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

    
</PropertyList>