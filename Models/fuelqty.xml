<?xml version="1.0"?>

<PropertyList>

 <path>fuelqty.ac</path>


 <animation>
  <type>material</type>
    <object-name>Fuelface</object-name>
    <object-name>FQneedle</object-name>
    <global type="bool">true</global>
  <emission>
    <red>1.0</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>


 <animation>
  <type>material</type>
    <object-name>FuelPanel</object-name>
    <global type="bool">true</global>
  <emission>
    <red>0.2</red>
    <green>0.2</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<animation>
    <type>rotate</type>
    <object-name>FQneedle.1</object-name>
    <property>/consumables/fuel/tank/level-lbs</property>
    <interpolation>
      <entry>
        <ind>  0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>  2000.0</ind>
        <dep>240.0</dep>
      </entry>
</interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.0</x-m>
      <y-m>-0.045</y-m>
      <z-m>0</z-m>
    </center>
</animation>
<animation>
    <type>rotate</type>
    <object-name>FQneedle.1</object-name>
    <property>/consumables/fuel/tank[2]/level-lbs</property>
    <interpolation>
      <entry>
        <ind>  0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>  2000.0</ind>
        <dep>240.0</dep>
      </entry>
</interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.0</x-m>
      <y-m>-0.045</y-m>
      <z-m>0</z-m>
    </center>
</animation>

<animation>
    <type>rotate</type>
    <object-name>FQneedle</object-name>
    <property>/consumables/fuel/tank[1]/level-lbs</property>
    <interpolation>
      <entry>
        <ind>  0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>  2000.0</ind>
        <dep>240.0</dep>
      </entry>
</interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0.045</y-m>
      <z-m>0.0</z-m>
    </center>
</animation>
<animation>
    <type>rotate</type>
    <object-name>FQneedle</object-name>
    <property>/consumables/fuel/tank[3]/level-lbs</property>
    <interpolation>
      <entry>
        <ind>  0.0</ind>
        <dep>0.0</dep>
      </entry>
      <entry>
        <ind>  2000.0</ind>
        <dep>240.0</dep>
      </entry>
</interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0.045</y-m>
      <z-m>0.0</z-m>
    </center>
</animation>

  
</PropertyList>
