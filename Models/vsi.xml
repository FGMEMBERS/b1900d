<?xml version="1.0"?>

<PropertyList>

 <path>vsi.ac</path>

  
   <animation>
  <type>material</type>
    <object-name>VSIface</object-name>
    <global type="bool">true</global>
  <emission>
    <red>1.0</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

  <animation>
    <type>rotate</type>
    <object-name>VSI.needle</object-name>
    <property>/autopilot/internal/vert-speed-fpm</property>
    <interpolation>
      <entry>
        <ind>  -4000.0</ind>
        <dep>-173.0</dep>
      </entry>
      <entry>
        <ind>  -1000.0</ind>
        <dep> -76.0</dep>
      </entry>
      <entry>
        <ind>      0.0</ind>
        <dep>   0.0</dep>
      </entry>
      <entry>
        <ind>   1000.0</ind>
        <dep>  76.0</dep>
      </entry>
      <entry>
        <ind>   4000.0</ind>
        <dep> 173.0</dep>
      </entry>
    </interpolation>
    <axis>
      <x>-1.0</x>
      <y>0</y>
      <z>0.0</z>
    </axis>
  </animation>

</PropertyList>
